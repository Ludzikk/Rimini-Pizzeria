<!DOCTYPE html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Rimini Pizzeria - Galeria</title>
		<link rel="shortcut icon" href="./dist/img/favicon.png" type="image/x-icon">
		<link rel="preconnect" href="https://fonts.googleapis.com" />
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
		<link
			href="https://fonts.googleapis.com/css2?family=Inter:wght@400;700&family=Roboto:wght@400;700&display=swap"
			rel="stylesheet"
		/>
		<link rel="preconnect" href="https://fonts.googleapis.com" />
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
		<link
			href="https://fonts.googleapis.com/css2?family=Kenia&display=swap"
			rel="stylesheet"
		/>
		<link href="./dist/css/hamburgers/hamburgers.min.css" rel="stylesheet" />
		<link rel="stylesheet" href="./dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
		<!-- @include "./nav.kit" -->
		<main class="main">
			<section class="main__section">
				<div class="wrapper">
					<div class="galery">
						<h2 class="section__title">Galeria</h2>
						<div class="galery__container">
							<div class="galery__imgcontainer">

								<img
									class="galery__img"
									src="./dist/img/mainpic1.jpg"
									alt="Restauracja na zewnątrz"
								/>
							</div>
							<div class="galery__imgcontainer">

								<img
									class="galery__img"
									src="./dist/img/mainpic2.jpg"
									alt="Restauracja w środku"
								/>
							</div>
							<div class="galery__imgcontainer">

								<img
									class="galery__img"
									src="./dist/img/pizza1.jpg"
									alt="Pizza na stole"
								/>
							</div>
							<div class="galery__imgcontainer">

								<img
									class="galery__img"
									src="./dist/img/pizza2.jpg"
									alt="Pizza przy piecu"
								/>
							</div>
							<div class="galery__imgcontainer">

								<img
									class="galery__img"
									src="./dist/img/pizza6.jpg"
									alt="Dwie pizze na stole"
								/>
							</div>
							<div class="galery__imgcontainer">

								<img
									class="galery__img"
									src="./dist/img/pizza5.jpg"
									alt="Stoliki z krzesłami na zewnątrz resauracji"
								/>
							</div>
							<div class="galery__imgcontainer">

								<img
									class="galery__img"
									src="./dist/img/pizza4.jpg"
									alt="Kucharz wsadzający pizze do pieca"
								/>
							</div>
							<div class="galery__imgcontainer">
								
								<img
									class="galery__img"
									src="./dist/img/pizza3.jpg"
									alt="Pizza z ciągnacym sie serem"
								/>
							</div>
						</div>
					</div>
				</div>
			</section>
		</main>
<div class="img hidden">
	<div class="img__bg"></div>
	<img src="./dist/img/mainpic1.jpg" alt="" class="img__pic">
</div>
		<!-- @include "./footer.kit" -->
		<script src="./dist/js/footeryear.min.js"></script>
		<script src="./dist/js/checknavstate.min.js"></script>
		<script src="./dist/js/galery.min.js"></script>
		<script src="./dist/js/togglenav.min.js"></script>
	</body>
</html>
