<!DOCTYPE html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Rimini Pizzeria - Galeria</title>
		<link rel="shortcut icon" href="./dist/img/favicon.png" type="image/x-icon">
		<link rel="preconnect" href="https://fonts.googleapis.com" />
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
		<link
			href="https://fonts.googleapis.com/css2?family=Inter:wght@400;700&family=Roboto:wght@400;700&display=swap"
			rel="stylesheet"
		/>
		<link rel="preconnect" href="https://fonts.googleapis.com" />
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
		<link
			href="https://fonts.googleapis.com/css2?family=Kenia&display=swap"
			rel="stylesheet"
		/>
		<link href="./dist/css/hamburgers/hamburgers.min.css" rel="stylesheet" />
		<link rel="stylesheet" href="./dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
		<!-- @include "./nav.kit" -->
		<main class="main">
			<section class="main__section">
				<div class="wrapper">
					<div class="menu">
						<!-- <h2 class="section__title">Menu</h2> -->
						<h3 class="menu__food">Pizza</h3>
						<div class="menu__container">
							<ul class="menu__list">
								<li class="menu__item">
									<div class="menu__left">
										<img src="./dist/img/menupizza1.jpg" alt="Pizza" class="menu__img">
									</div>
									<div class="menu__right">
										<p class="menu__title">1. margherita</p>
										<p class="menu__text">sos pomidorowy, ser</p>
										<p class="menu__price">18 zł</p>
										<button class="menu__btn">Zamów</button>
									</div>
								</li>
								<li class="menu__item">
									<div class="menu__left">
										<img src="./dist/img/menupizza2.jpg" alt="Pizza" class="menu__img">
									</div>
									<div class="menu__right">
										<p class="menu__title">2. diabelska</p>
										<p class="menu__text">sos pomidorowy, ser, pieczarki, salami, szynka, papryka peperoni, chilli</p>
										<p class="menu__price">24 zł</p>
										<button class="menu__btn">Zamów</button>
									</div>
								</li>
								<li class="menu__item">
									<div class="menu__left">
										<img src="./dist/img/pizza1.jpg" alt="Pizza" class="menu__img">
									</div>
									<div class="menu__right">
										<p class="menu__title">3. wiejska</p>
										<p class="menu__text">sos pomidorowy, ser, pieczarki, kiełbasa, szynka, baleron</p>
										<p class="menu__price">20 zł</p>
										<button class="menu__btn">Zamów</button>
									</div>
								</li>
								<li class="menu__item">
									<div class="menu__left">
										<img src="./dist/img/pizza2.jpg" alt="Pizza" class="menu__img">
									</div>
									<div class="menu__right">
										<p class="menu__title">4. serowa</p>
										<p class="menu__text">sos pomidorowy, ser, feta, pomidor, mozzarella</p>
										<p class="menu__price">19 zł</p>
										<button class="menu__btn">Zamów</button>
									</div>
								</li>
							</ul>
						</div>
					</div>
				</div>
			</section>
			<section class="main__section main__section--bgtwo">
				<div class="wrapper">
					<div class="menu">
						<h3 class="menu__food">Frytki</h3>
						<div class="menu__container">
							<ul class="menu__list">
								<li class="menu__item">
									<div class="menu__left">
										<img src="./dist/img/frytki2.jpg" alt="Frytki" class="menu__img">
									</div>
									<div class="menu__right">
										<p class="menu__title">Frytki(140g)</p>
										<p class="menu__text">sos pomidorowy, ser</p>
										<p class="menu__price">12 zł</p>
										<button class="menu__btn">Zamów</button>
									</div>
								</li>
								<li class="menu__item">
									<div class="menu__left">
										<img src="./dist/img/frytki1.jpg" alt="Ziemniaczki" class="menu__img">
									</div>
									<div class="menu__right">
										<p class="menu__title">Ziemnaczki(120g)</p>
										<p class="menu__text">sos pomidorowy, ser, pieczarki, salami, szynka, papryka peperoni, chilli</p>
										<p class="menu__price">13 zł</p>
										<button class="menu__btn">Zamów</button>
									</div>
								</li>
							</ul>
						</div>
					</div>
				</div>
			</section>
			<section class="main__section">
				<div class="wrapper">
					<div class="menu">
						<h3 class="menu__food">Dodatki Napoje</h3>
						<div class="menu__container">
							<ul class="menu__list">
								<li class="menu__item">
									<div class="menu__left">
										<img src="./dist/img/sos.jpg" alt="Frytki" class="menu__img">
									</div>
									<div class="menu__right">
										<p class="menu__title">Sos czosnkowy, keczup</p>
										
										<p class="menu__price">2 zł</p>
										<button class="menu__btn">Zamów</button>
									</div>
								</li>
								<li class="menu__item">
									<div class="menu__left">
										<img src="./dist/img/pepsi.jpg" alt="Ziemniaczki" class="menu__img">
									</div>
									<div class="menu__right">
										<p class="menu__title">Pepsi, sprite, lipton(250ml)</p>
										<p class="menu__price">8 zł</p>
										<button class="menu__btn">Zamów</button>
									</div>
								</li>
							</ul>
						</div>
					</div>
				</div>
			</section>
		</main>

		<!-- @include "./footer.kit" -->
		<script src="./dist/js/footeryear.min.js"></script>
		<script src="./dist/js/checknavstate.min.js"></script>
		<script src="./dist/js/togglenav.min.js"></script>
	</body>
</html>
